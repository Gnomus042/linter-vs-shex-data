    "recipeIngredient": [
        {
            "@language": "en",
            "@value": "\n                                                                100g 3-day-old white bread, crusts removed\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                25g plain flour\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                75g Smart Price Cooked Ham Slices, shredded\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                1 egg, lightly beaten\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                Vegetable oil, to fry\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                180ml semi-skimmed milk\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                25g Smart Price Butter\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                25g Smart Price Mature Cheddar, grated\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                1tsp course-grain mustard\n                                                            "
        }
    ],
    "recipeInstructions": [
        {
            "@language": "en",
            "@value": "Melt the butter in a small pan. Remove from the heat and stir in the flour to make a smooth paste. Heat the milk until steaming hot, then gradually whisk it into the mixture. Cook over a medium heat, whisking all the time, until the mixture thickens. Continue to whisk for another 3 mins."
        },
        {
            "@language": "en",
            "@value": "Remove from the heat and stir in the cheese, ham and mustard. Season with black pepper. Pour into a dish and cover with clingfilm. Allow to cool completely, then chill in the fridge for 1 hr."
        },
        {
            "@language": "en",
            "@value": "Fill a deep fat fryer or large pan to 1\u20443 of the way up with oil and heat to 180C. Cook the croquettes, in 2 batches, for 2-3 mins until golden. Drain on a plate lined with kitchen roll, keep warm, then serve 2 croquettes per person."
        },
        {
            "@language": "en",
            "@value": "Preheat the oven to 140C/ 120C Fan/Gas 1. Break the bread into small pieces and whizz in a processor to make breadcrumbs. Spread out on a baking tray and cook for 10 mins. Cool."
        },
        {
            "@language": "en",
            "@value": "Put the egg in a shallow dish and the breadcrumbs in another. Shape the cheese and ham mixture into 8 croquettes. One at a time, coat in the egg and then the breadcrumbs. Put on a plate lined with baking paper and chill for 30 mins."
        }
    ],
