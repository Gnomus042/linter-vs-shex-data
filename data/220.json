    "ingredients": [
        {
            "@language": "en-US",
            "@value": "\u00bd cup jelly jam (any flavor, 1 tsp per muffin)"
        },
        {
            "@language": "en-US",
            "@value": "1 cup powdered peanut butter (loosely measured, not packed down)"
        },
        {
            "@language": "en-US",
            "@value": "2 eggs"
        },
        {
            "@language": "en-US",
            "@value": "\u00bd tsp salt"
        },
        {
            "@language": "en-US",
            "@value": "1 cup nonfat plain Greek yogurt"
        },
        {
            "@language": "en-US",
            "@value": "\u00bd cup white sugar"
        },
        {
            "@language": "en-US",
            "@value": "1 tsp baking soda"
        },
        {
            "@language": "en-US",
            "@value": "1\u00bd cups mashed overripe bananas (I freeze my bananas when they get too ripe and then thaw them out for recipes)"
        },
        {
            "@language": "en-US",
            "@value": "1 tsp vanilla extract"
        },
        {
            "@language": "en-US",
            "@value": "2 cups gluten free flour (I use Beyond the Grain Gluten Free)"
        },
        {
            "@language": "en-US",
            "@value": "1\u00bd tsp baking powder"
        },
        {
            "@language": "en-US",
            "@value": "\u00bc tsp ground cinnamon"
        }
    ],
    "name": {
        "@language": "en-US",
        "@value": "Peanut Butter and Jelly Muffins {Gluten Free}"
    },
