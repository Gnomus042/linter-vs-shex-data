{
    "@type": "Recipe",
    "author": {
        "@language": "en-US",
        "@value": "Anti Aging Company"
    },
    "description": {
        "@language": "en-US",
        "@value": "Fruit-based desserts can contribute to making half your plate fruits and veggies. True to their name, Honeycrisp apples are sweet and crunchy, perfectly delicious raw, but also ideal in baking. And their sweetness lends potential to reducing added sugar, as we did with this apple pistachio crisp."
    },
    "image": {
        "@id": "http://blog.antiagingcompany.com/wp-content/uploads/2014/11/apple-pistachio-crisp-300x300.jpg"
    },
    "ingredients": [
        {
            "@language": "en-US",
            "@value": "1 tsp cinnamon"
        },
        {
            "@language": "en-US",
            "@value": "2 tbsp margarine or butter, melted"
        },
        {
            "@language": "en-US",
            "@value": "3 apples (such as Honeycrisp), cored and cut into 1\u201d chunks (unpeeled)"
        },
        {
            "@language": "en-US",
            "@value": "\u00bd cup raisins"
        },
        {
            "@language": "en-US",
            "@value": "Juice from \u00bd lemon"
        },
        {
            "@language": "en-US",
            "@value": "\u00bc cup or about 2 ounces pistachios, unsalted, chopped"
        },
        {
            "@language": "en-US",
            "@value": "\u00bc cup whole-wheat flour"
        },
        {
            "@language": "en-US",
            "@value": "\u00bd cup old fashioned oats"
        },
        {
            "@language": "en-US",
            "@value": "3 tbsp brown sugar"
        }
    ],
    "name": {
        "@language": "en-US",
        "@value": "Apple Pistachio Crisp"
    },
    "recipeCategory": {
        "@language": "en-US",
        "@value": "Dinner"
    },
    "recipeCuisine": {
        "@language": "en-US",
        "@value": "American"
    },
    "recipeInstructions": [
        {
            "@language": "en-US",
            "@value": "In a bowl, mix remaining ingredients except melted margarine."
        },
        {
            "@language": "en-US",
            "@value": "Bake uncovered 45-50 minutes or until apples are tender."
        },
        {
            "@language": "en-US",
            "@value": "Place sliced apples, raisins, and lemon juice in 8\u201d x 8\u201d pan or baking dish; toss."
        },
        {
            "@language": "en-US",
            "@value": "Place rack in center of oven and preheat to 350 \u00baF."
        },
        {
            "@language": "en-US",
            "@value": "Add melted margarine and mix until texture is consistent. Sprinkle over apple-raisin mixture."
        }
    ],
    "recipeYield": {
        "@language": "en-US",
        "@value": "4 servings"
    },
    "@id": "http://example.org/recipe"
}