    "recipeInstructions": [
        {
            "@language": "pl",
            "@value": "Dodajemy, oczyszczon\u0105, umyt\u0105 i na cienkie paseczki pokrojon\u0105 papryk\u0119. Dusimy do mi\u0119kko\u015bci."
