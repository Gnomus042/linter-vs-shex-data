    "recipeIngredient": [
        {
            "@value": "1 tsp Turmeric"
        },
        {
            "@value": "3 piece Carrots"
        },
        {
            "@value": "1 tsp Olive Oil"
        },
        {
            "@value": "1/2 piece Apple"
        },
        {
            "@value": "4 tbsp Coconut Milk"
        },
        {
            "@value": "2 piece large Fennels"
        }
    ],
    "recipeInstructions": {
        "@type": "ItemList",
        "itemListElement": [
            {
                "@value": "Enjoy."
            },
            {
                "@value": "Thinly cut the apple, grate the carrots and top them over the fennels."
            },
            {
                "@value": "Wash the fennels and cut them widthwise. Grease a casserole pan with olive oil and put the sliced fennels in it and dust them with turmeric."
            },
            {
                "@value": "Preheat the oven to 380 F"
            },
            {
                "@value": "Sprinkle the coconut milk on top and bake for about 20 minutes."
            },
            {
                "@value": "Flip the fennels upside down, mix them well, put a cover on the casserole and bake for 20 more minutes."
            }
        ],
