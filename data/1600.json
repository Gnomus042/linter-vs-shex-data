    "recipeIngredient": [
        {
            "@language": "en-us",
            "@value": "1 pound ground turkey"
        },
        {
            "@language": "en-us",
            "@value": "1/2 cup chopped green bell pepper"
        },
        {
            "@language": "en-us",
            "@value": "1 (14.5 ounce) can diced tomatoes"
        },
        {
            "@language": "en-us",
            "@value": "1/2 cup shredded Cheddar cheese"
        },
        {
            "@language": "en-us",
            "@value": "1 tablespoon vegetable oil"
        },
        {
            "@language": "en-us",
            "@value": "2 cups pumpkin puree"
        },
        {
            "@language": "en-us",
            "@value": "1/2 teaspoon ground black pepper"
        },
        {
            "@language": "en-us",
            "@value": "1/2 cup sour cream"
        },
        {
            "@language": "en-us",
            "@value": "1/2 cup chopped yellow bell pepper"
        },
        {
            "@language": "en-us",
            "@value": "1 clove garlic, minced"
        },
        {
            "@language": "en-us",
            "@value": "1 1/2 tablespoons chili powder"
        },
        {
            "@language": "en-us",
            "@value": "1 cup chopped onion"
        },
        {
            "@language": "en-us",
            "@value": "1 dash salt"
        }
    ],
    "recipeInstructions": {
        "@language": "en-us",
        "@value": "\n                        \n                            Heat the oil in a large skillet over medium heat, and saute the onion, green bell pepper, yellow bell pepper, and garlic until tender. Stir in the turkey, and cook until evenly brown. Drain, and mix in tomatoes and pumpkin. Season with chili powder, pepper, and salt. Reduce heat to low, cover, and simmer 20 minutes. Serve topped with Cheddar cheese and sour cream.\n                            \n                        \n            "
    },
