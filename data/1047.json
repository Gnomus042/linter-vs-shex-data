    "recipeInstructions": [
        {
            "@language": "en-US",
            "@value": "Preheat oven to 350 degrees."
        },
        {
            "@language": "en-US",
            "@value": "Using an electric or stand mixer, beat together butter and both sugars until creamy. Add egg, egg yolk, and vanilla. Beat until combined."
        },
        {
            "@language": "en-US",
            "@value": "Add in baking soda, baking powder, salt, cocoa powder and 1\u00bc cups of the flour, and beat slowly until combined. Add in last cup of flour and milk, and beat until thoroughly combined."
        },
        {
            "@language": "en-US",
            "@value": "Bake cookies for 10 minutes. Let cool a few minutes on the baking sheets, then move to a cooling rack."
        },
        {
            "@language": "en-US",
            "@value": "Add the baking chips and mix on low a few seconds until they are incorporated into the dough."
        },
        {
            "@language": "en-US",
            "@value": "Place rounded tablespoons of dough on a baking sheet, about an inch apart."
        }
    ],
