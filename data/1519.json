    "recipeIngredient": [
        {
            "@language": "en",
            "@value": "\n                                                                120g Extra Special Speyburn Single Malt Scotch Whisky Scottish Smoked Salmon, torn\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                Lemon wedges\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                2 balls Asda Pickled Beetroot, sliced\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                2tbsp rapeseed oil\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                225g Grower's Selection Vine Ripened Cherry Tomatoes\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                1 small onion\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                300g Maris Piper potatoes\n                                                            "
        }
    ],
    "recipeInstructions": [
        {
            "@language": "en",
            "@value": "Preheat the oven to 200C/180C Fan/Gas 6. Roast the tomatoes on a baking tray for 15 mins."
        },
        {
            "@language": "en",
            "@value": "Top with the pickled beetroot, smoked salmon and black pepper. Serve with the tomatoes and lemon wedges."
        },
        {
            "@language": "en",
            "@value": "Heat the oil in a nonstick frying pan. Add the potato mixture; flatten. Fry for 6 mins each side until crisp. Drain on kitchen roll."
        },
        {
            "@language": "en",
            "@value": "Peel and grate the potatoes and onion into a clean tea towel; squeeze out excess liquid."
        }
    ],
