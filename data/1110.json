    "recipeInstructions": [
        {
            "@language": "en-US",
            "@value": "Add leeks saut\u00e9 until softened. Add asparagus saut\u00e9 for about a minute."
        },
        {
            "@language": "en-US",
            "@value": "Process until smooth and set aside to stay cold put in the refrigerator until ready to serve."
        },
        {
            "@language": "en-US",
            "@value": "Add milk and mix thoroughly."
        },
        {
            "@language": "en-US",
            "@value": "Put in Oven uncovered, bake for 20 minutes or until set."
        },
        {
            "@language": "en-US",
            "@value": "In large oven proof skillet, heat the olive oil on Medium High heat."
        },
        {
            "@language": "en-US",
            "@value": "Let rest for 5 minutes before cutting in sections. Servings 4 \u2013 6."
        },
        {
            "@language": "en-US",
            "@value": "In bowl, whisk the eggs until smooth."
        },
        {
            "@language": "en-US",
            "@value": "Drizzle a little Extra Virgin Olive oil over the greens."
        },
        {
            "@language": "en-US",
            "@value": "Oven 375 degrees, preheat"
        },
        {
            "@language": "en-US",
            "@value": "Put all ingredients in a food processor or blender."
        },
        {
            "@language": "en-US",
            "@value": "Add bread cubes let soak for 3 minutes."
        },
        {
            "@language": "en-US",
            "@value": "Quiche:"
        },
        {
            "@language": "en-US",
            "@value": "Store any unused portions up to 3 days."
        },
        {
            "@language": "en-US",
            "@value": "Caraway Tofu Cream:"
        },
        {
            "@language": "en-US",
            "@value": "Add egg mixture to leeks and asparagus in skillet. Reduce heat to Medium and stir in Cheese and Cabbage."
        },
        {
            "@language": "en-US",
            "@value": "Place quiche on top of tender greens then top the quiche with 1 Tablespoon of Caraway Tofu Cream. Grind a bit of Makai Celtic Sea Salt and Fresh Ground Pepper on top seasoning to taste."
        }
    ],
