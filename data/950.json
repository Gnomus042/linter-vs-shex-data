    "recipeIngredient": [
        {
            "@language": "en",
            "@value": "\n                                                                3 medium aubergines\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                1 level tsp sugar\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                5 tbsp olive oil\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                1 small onion, chopped\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                20 basil leaves\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                50g Parmesan, grated\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                2 garlic cloves, crushed\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                2 x 400g cans Asda Chopped Tomatoes\n                                                            "
        },
        {
            "@language": "en",
            "@value": "\n                                                                2 x 125g balls Asda Mozzarella, drained and sliced\n                                                            "
        }
    ],
    "recipeInstructions": [
        {
            "@language": "en",
            "@value": "Sprinkle on the Parmesan and bake for 40 minutes. Garnish with basil to serve."
        },
        {
            "@language": "en",
            "@value": "Pre-heat the oven to 180C/160C Fan/Gas 4. Cook the onion in 1 tbsp oil until soft. Add the garlic and cook for 1 minute, then add the tomatoes and sugar. Simmer, uncovered, for 20 minutes or until the sauce is thick. Shred 12 basil leaves and stir in."
        },
        {
            "@language": "en",
            "@value": "Layer the tomato sauce, aubergines and mozzarella in an ovenproof dish."
        },
        {
            "@language": "en",
            "@value": "Cut the aubergines into slices 1/2cm thick, brush both sides with oil and griddle until just starting to brown."
        }
    ],
