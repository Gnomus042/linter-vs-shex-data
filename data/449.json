    "recipeIngredient": [
        {
            "@language": "en",
            "@value": "\n                                                                    cannellini beans 400g,\n                                                                    drained and rinsed\n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    balsamic vinegar 1 tbsp\n                                                                    \n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    rocket 1 handful\n                                                                    \n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    cherry tomatoes a handful,\n                                                                    halved\n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    red onion \u00bc,\n                                                                    thinly sliced\n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    flat-leaf parsley a few leaves,\n                                                                    torn\n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    mint a few leaves,\n                                                                    torn\n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    lemon \u00bd,\n                                                                    zested and juiced\n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    vegetable oil 2 tsp\n                                                                    \n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    green beans 150g\n                                                                    \n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    tuna steaks 2\n                                                                    \n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    preserved lemon 1,\n                                                                    pulp removed and discarded, skin finely chopped\n                                                                "
        },
        {
            "@language": "en",
            "@value": "\n                                                                    extra-virgin olive oil 1 tbsp\n                                                                    \n                                                                "
        }
    ],
    "recipeInstructions": {
        "@type": "ItemList",
        "itemListElement": [
            {
                "@language": "en",
                "@value": "\n                        Step 3\n                        \n                            Heat a griddle or frying pan to hot, rub the oil into the tuna steaks and season generously. Once the pan is scorching, cook the tuna steaks for 30 seconds on each side.\n\n                        \n                    "
            },
            {
                "@language": "en",
                "@value": "\n                        Step 4\n                        \n                            Toss the rocket, mint and parsley with the salad, serve, and top with the tuna steaks.\n\n                        \n                    "
            },
            {
                "@language": "en",
                "@value": "\n                        Step 2\n                        \n                            Whisk together all of the dressing ingredients with some seasoning in a bowl and add the cannellini beans, onion and cherry tomatoes. Cut the green beans in half, add to the bowl and toss well.\n\n                        \n                    "
            },
            {
                "@language": "en",
                "@value": "\n                        Step 1\n                        \n                            Put the green beans in a large pan of simmering salted water for 1-2 minutes, then drain and refresh in ice-cold water.\n\n                        \n                    "
            }
        ]
    },
