    "recipeIngredient": [
        {
            "@value": "1 cup long grain rice (uncooked)"
        },
        {
            "@value": "2 eggs, beaten"
        },
        {
            "@value": "1 onion, chopped"
        },
        {
            "@value": "2 T cooking oil"
        },
        {
            "@value": "2 cups chicken broth"
        },
        {
            "@value": "2 thin cut boneless pork chops"
        },
        {
            "@value": "More soy sauce and chopped green onions for serving"
        },
        {
            "@value": "1 12oz bag frozen mixed vegetables"
        },
        {
            "@value": "1 tsp garlic powder"
        },
        {
            "@value": "2 T soy sauce"
        },
        {
            "@value": "1/8 tsp black pepper"
        }
    ],
    "recipeInstructions": [
        {
            "@value": "Put rice in skillet, stir and let toast slightly. Pour in broth, cover and reduce heat to a simmer for 20 minutes."
        },
        {
            "@value": "Mix together soy sauce, garlic powder and pepper. Cut the pork chops into very small pieces and soak in the soy sauce marinade."
        },
        {
            "@value": "Add more soy sauce to taste and top with green onions"
        },
        {
            "@value": "Meanwhile heat a skillet and the oil over medium heat. Add the onion and saute until they begin to soften."
        },
        {
            "@value": "Push the rice to the edges of the skillet and pour the eggs into the center. Once they are set, stir them into the eggs."
        },
        {
            "@value": "Add pork and soy sauce mixture and cook until pork is cooked through."
        },
        {
            "@value": "Stir in the mixed vegetables and cook until softened."
        }
    ],
