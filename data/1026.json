    "recipeInstructions": [
        {
            "@language": "en",
            "@value": "Add rice, cinnamon, black pepper, water,MAGGI\u00ae Chicken Bouillon and raisins. Cook over low heat for 10 mins or until rice is almost cooked."
        },
        {
            "@language": "en",
            "@value": "Stuff the chicken with as much rice as possible and continue cooking the remaining rice to be served with the chicken."
        },
        {
            "@language": "en",
            "@value": "Roast the chicken in a 190\u00b0C preheated oven for 45-50 mins or until chicken is cooked."
        },
        {
            "@language": "en",
            "@value": "Heat oil in a medium saucepan, add and cook onions and pine seeds over medium heat for 4-5 mins or until almost golden."
        },
        {
            "@language": "en",
            "@value": "Serve roasted chicken with the rest of rice and fresh green salad."
        }
