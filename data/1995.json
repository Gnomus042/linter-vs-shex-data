    "recipeIngredient": [
        {
            "@language": "en-US",
            "@value": "\n                  \n                    2\n                  \n                  \n                    tablespoons freshly grated Parmesan cheese\n                  \n                "
        },
        {
            "@language": "en-US",
            "@value": "\n                  \n                    \n                  \n                  \n                    Freshly ground pepper to taste\n                  \n                "
        },
        {
            "@language": "en-US",
            "@value": "\n                  \n                    \u00bd\n                  \n                  \n                    cup flour\n                  \n                "
        },
        {
            "@language": "en-US",
            "@value": "\n                  \n                    6\n                  \n                  \n                    cups corn, peanut or vegetable oil\n                  \n                "
        },
        {
            "@language": "en-US",
            "@value": "\n                  \n                    \u00bd\n                  \n                  \n                    teaspoon finely chopped fresh rosemary, or half that amount dried\n                  \n                "
        },
        {
            "@language": "en-US",
            "@value": "\n                  \n                    3\n                  \n                  \n                    tablespoons cold water\n                  \n                "
        },
        {
            "@language": "en-US",
            "@value": "\n                  \n                    \n                  \n                  \n                    Salt to taste, if desired\n                  \n                "
        },
        {
            "@language": "en-US",
            "@value": "\n                  \n                    2\n                  \n                  \n                    cups fine fresh bread crumbs\n                  \n                "
