    "recipeIngredient": [
        {
            "@language": "en-us",
            "@value": "1 cup long grain rice"
        },
        {
            "@language": "en-us",
            "@value": "1/2 small onion, diced"
        },
        {
            "@language": "en-us",
            "@value": "fresh ground black pepper"
        },
        {
            "@language": "en-us",
            "@value": "1/2 teaspoon salt"
        },
        {
            "@language": "en-us",
            "@value": "2 tablespoons olive oil"
        },
        {
            "@language": "en-us",
            "@value": "2 tablespoons chopped fresh parsley"
        },
        {
            "@language": "en-us",
            "@value": "1/2 cup tomato sauce"
        },
        {
            "@language": "en-us",
            "@value": "1 1/2 cups chicken broth"
        },
        {
            "@language": "en-us",
            "@value": "2 cloves garlic"
        }
    ],
    "recipeInstructions": {
        "@language": "en-us",
        "@value": "\n                        \n                            Heat oil in a saucepan over medium heat and cook onion and garlic until soft and translucent, about 3 minutes. Add rice; cook and stir until lightly toasted for 2 minutes. Add chicken broth, tomato sauce, parsley, salt, and pepper; bring to a boil.\n                            \n                        \n                        \n                            Cover and reduce heat to low. Cook until all the liquid is absorbed and rice is done, about 20 minutes.\n                            \n                        \n            "
    },
